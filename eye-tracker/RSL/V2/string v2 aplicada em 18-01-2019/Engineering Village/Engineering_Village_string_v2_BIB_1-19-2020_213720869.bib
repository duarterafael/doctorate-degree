copyright = {Compilation and indexing terms, Copyright 2020 Elsevier Inc.},
copyright = {Compendex},
title = {Eye-tracking the factors of process model comprehension tasks},
journal = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
author = {Petrusel, Razvan and Mendling, Jan},
volume = {7908 LNCS},
year = {2013},
pages = {224 - 239},
issn = {03029743},
address = {Valencia, Spain},
abstract = {Understanding business process models has been previously related to various factors. Those factors were determined using statistical approaches either on model repositories or on experiments based on comprehension questions. We noticed that, when asking comprehension questions on a process model, usually the expert explores only a part of the entire model to provide the answer. This paper formalizes this observation under the notion of Relevant Region. We conduct an experiment using eye-tracking to prove that the Relevant Region is indeed correlated to the answer given to the comprehension question. We also give evidence that it is possible to predict whether the correct answer will be given to a comprehension question, knowing the number and the time spent fixating Relevant Region elements. This paper sets the foundations for future improvements on model comprehension research and practice. &copy; 2013 Springer-Verlag.<br/>},
key = {Eye tracking},
keywords = {Information systems;Information use;Systems engineering;},
note = {Business process model;Comprehension tasks;Future improvements;Model repositories;Process Modeling;Statistical approach;Time spent;},
URL = {http://dx.doi.org/10.1007/978-3-642-38709-8_15},
} 


