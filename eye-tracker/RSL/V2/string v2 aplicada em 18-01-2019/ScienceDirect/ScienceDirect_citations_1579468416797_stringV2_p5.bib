@incollection{BALOTA2006285,
title = "Chapter 9 - Visual Word Recognition: The Journey from Features to Meaning (A Travel Update)",
editor = "Matthew J. Traxler and Morton A. Gernsbacher",
booktitle = "Handbook of Psycholinguistics (Second Edition)",
publisher = "Academic Press",
edition = "Second Edition",
address = "London",
pages = "285 - 375",
